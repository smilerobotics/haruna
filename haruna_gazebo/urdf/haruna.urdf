  <link name="left_leg_link_1">
    <inertial>
      <origin xyz="0.0 0.10865606936416185 -0.19859393063583816" rpy="0 0 0"/>
      <mass value="0.06790249999999999"/>
      <inertia ixx="3.8e-05" iyy="3.8e-05" izz="2e-06" ixy="0.0" iyz="2e-06" ixz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="-0.0 -0.0 -0.313" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://haruna_gazebo/meshes/left_leg_link_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="silver">
        <color rgba="0.700 0.700 0.700 1.000"/>
      </material>
    </visual>
    <collision>
      <origin xyz="-0.0 -0.0 -0.313" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://haruna_gazebo/meshes/left_leg_link_1.stl" scale="0.001 0.001 0.001"/>
